generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String
  profilePicture String
  lastLogin DateTime?
  registeredAt DateTime @default(dbgenerated())
}
